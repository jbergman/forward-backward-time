	Journal = {Mol Biol Evol},
	Journal-Full = {Molecular biology and evolution},
	Keywords = {Drosophila simulans; Drosophila yakuba; deletions; evolutionary novelty; tandem duplications},
	Mesh = {Animals; Drosophila; Evolution, Molecular; Female; Gene Duplication; Genetic Variation; Genome; Genotype; Mutation Rate; Sequence Deletion; Tandem Repeat Sequences},
	Month = {Jul},
	Number = {7},
