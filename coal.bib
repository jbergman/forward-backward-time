	Pmc = {PMC4069613},
	Pmid = {24710518},
	Pst = {ppublish},
	Title = {Landscape of standing variation for tandem duplications in Drosophila yakuba and Drosophila simulans},
	Volume = {31},
	Year = {2014}}

